{
  "name": "copy-tab",
  "publisher": "AlexZanfir",
  "displayName": "Copy Tab",
  "description": "Copy the content of all your open tabs. It's an ideal tool for quickly sharing code, transferring work between files, or pasting your code directly into GPT chat for AI-powered assistance.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.copyAllOpenFiles",
        "title": "CopyTab: All tabs"
      },
      {
        "command": "extension.copyActiveTab",
        "title": "CopyTab: Active tab"
      }
    ]
  },
  "scripts": {
    "watch": "tsc -w",
    "pretest": "npm run compile",
    "compile": "tsc -p ./",
    "vscode:prepublish": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.3",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.14.31",
    "@types/vscode": "^1.52.0",
    "mocha": "^8.4.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.4.0",
    "glob": "^7.1.6",
    "@vscode/test-electron": "^1.4.0",
    "@types/glob": "^7.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexZan/copy-tab"
  }
}